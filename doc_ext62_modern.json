        "events": [
            {
                "name": "beforeload",
                "doc": "Fires before a request is made for a new data object. If the beforeload handler returns false the load\naction will be canceled.\n"
            },
            {
                "name": "beforenodecollapse",
                "doc": "Fires before an row is visually collapsed. May be vetoed by returning false from a handler.\n"
