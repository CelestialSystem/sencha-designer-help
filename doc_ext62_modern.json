            {
                "name": "DOMSubtreeModified",
                "doc": "Where supported. Fires when the subtree is modified.\n"
            },
            {
                "name": "doubletap",
                "doc": "Fires when there is a double tap.\n"
            },
            {
                "name": "error",
                "doc": "Fires when an object/image/frame cannot be loaded properly.\n"
            },
            {
                "name": "focus",
                "doc": "Fires when an element receives focus either via the pointing device or by tab navigation.\n"
            },
            {
                "name": "focusmove",
                "doc": "Fires when focus is moved *within* an element.\n"
            },
            {
                "name": "keydown",
                "doc": "Fires when a keydown is detected within the element.\n"
