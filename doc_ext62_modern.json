    "Ext.draw.plugin.SpriteEvents": {
        "type": "Ext.draw.plugin.SpriteEvents",
        "doc": "A draw container {@link Ext.plugin.Abstract plugin} that adds ability to listen\nto sprite events. For example:\n\n    var drawContainer = Ext.create('Ext.draw.Container', {\n         plugins: ['spriteevents'],\n         renderTo: Ext.getBody(),\n         width: 200,\n         height: 200,\n         sprites: [{\n              type: 'circle',\n              fillStyle: '#79BB3F',\n              r: 50,\n              x: 100,\n              y: 100\n         }],\n         listeners: {\n             spriteclick: function (item, event) {\n                 var sprite = item && item.sprite;\n                 if (sprite) {\n                     sprite.setAttributes({fillStyle: 'red'});\n                       sprite.getSurface().renderFrame();\n                 }\n             }\n         }\n    });\n",
        "configs": [],
        "events": [
            {
                "name": "spriteclick",
                "doc": "Fires when a click event occurs on a sprite.\n"
            },
            {
                "name": "spritedblclick",
                "doc": "Fires when a double click event occurs on a sprite.\n"
            },
            {
                "name": "spritemousedown",
                "doc": "Fires when a mousedown event occurs on a sprite.\n"
            },
            {
                "name": "spritemousemove",
                "doc": "Fires when the mouse is moved on a sprite.\n"
            },
            {
                "name": "spritemouseout",
                "doc": "Fires when the mouse exits a sprite.\n"
            },
            {
                "name": "spritemouseover",
                "doc": "Fires when the mouse enters a sprite.\n"
            },
            {
                "name": "spritemouseup",
                "doc": "Fires when a mouseup event occurs on a sprite.\n"
            },
            {
                "name": "spritetap",
                "doc": "Fires when a tap event occurs on a sprite.\n"
            }
        ]
    },
