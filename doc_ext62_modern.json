    "Ext.data.validator.Email": {
        "type": "Ext.data.validator.Email",
        "doc": "Validates that the value is a valid email.\n",
        "configs": [
            {
                "name": "matcher",
                "type": "RegExp",
                "doc": "A matcher to check for simple emails. This may be overridden.\n",
                "default": "/^(\")?(?:[^\\.\"])(?:(?:[\\.])?(?:[\\w\\-!#$%&'*+\\/=?\\^_`{|}~]))*\\1@(\\w[\\-\\w]*\\.){1,5}([A-Za-z]){2,6}$/"
            },
            {
                "name": "message",
                "type": "String",
                "doc": "The error message to return when the value is not a valid email\n",
                "default": "'Is not a valid email address'"
            }
