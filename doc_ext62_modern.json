    "Ext.grid.Tree": {
        "type": "Ext.grid.Tree",
        "configs": [
            {
                "name": "expanderOnly",
                "type": "Boolean",
                "doc": "`true` to expand only on the click of the expander element. Setting this to\n`false` will allow expansion on click of any part of the element.\n",
