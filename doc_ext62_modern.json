        ],
        "events": []
    },
    "Ext.data.validator.Range": {
        "type": "Ext.data.validator.Range",
        "doc": "Validates that the the value is between a {@link #cfg!min #min} and {@link #cfg!max #max}.\n",
        "configs": [
            {
                "name": "bothMessage",
                "type": "String",
                "doc": "The error message to return when the value is not in the specified range\nand both the minimum and maximum are specified.\n",
                "default": "'Must be between {0} and {1}'"
            },
            {
                "name": "max",
                "type": "Number",
                "doc": "The maximum value.\n"
            },
            {
