    "Ext.draw.gradient.Linear": {
        "type": "Ext.draw.gradient.Linear",
        "doc": "Linear gradient.\n\n    @example\n    Ext.create({\n       xtype: 'draw',\n       renderTo: document.body,\n       width: 600,\n       height: 400,\n       sprites: [{\n           type: 'circle',\n           cx: 100,\n           cy: 100,\n           r: 100,\n           fillStyle: {\n               type: 'linear',\n               degrees: 180,\n               stops: [{\n                   offset: 0,\n                   color: '#1F6D91'\n               }, {\n                   offset: 1,\n                   color: '#90BCC9'\n               }]\n           }\n       }]\n    });\n",
