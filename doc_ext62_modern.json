    "Ext.pivot.plugin.Configurator": {
        "type": "Ext.pivot.plugin.Configurator",
        "doc": "This plugin allows the end user to configure the pivot component.\n\nIt adds the following methods to the pivot grid:\n- showConfigurator: which when called will show the configurator panel\n- hideConfigurator: which when called will hide the configurator panel\n\nThe configurator panel will be shown when the end-user does a `longpress` event\non the column headers.\n",
        "configs": [
            {
                "name": "fields",
                "type": "Ext.pivot.plugin.configurator.Field[]",
                "doc": "\nThis is the array of fields you want to be used in the configurator.\n\nIf no fields are defined then all fields are fetched from the store model if\na {@link Ext.pivot.matrix.Local} matrix is used.\n\nThe fields are indexed by the dataIndex supplied to them which means that you can't have two fields\nsharing the same dataIndex. If you want to define two fields that share the same dataIndex then\nit's best to use a unique dataIndex for the 2nd field and define a grouperFn on it.\n\nThe dimensions that are configured on the pivot component but do not exist in this fields collection\nwill be added here with a set of default settings.\n",
                "default": "[]"
            },
            {
                "name": "panel",
                "type": "Object",
                "doc": "\nConfiguration object used to instantiate the configurator panel.\n",
                "default": "{\n    xtype: 'pivotconfigpanel',\n    docked: 'right'\n}"
            },
            {
                "name": "panelWrap",
                "type": "Boolean",
                "doc": "\nEnable or disable the configurator panel wrapper.\n",
                "default": "true"
            },
            {
                "name": "panelWrapper",
                "type": "Object",
                "doc": "\nConfiguration object used to wrap the configurator panel when needed.\n",
                "default": "{\n    xtype: 'sheet',\n    right: 0,\n    stretchY: true,\n    layout: 'fit',\n    hideOnMaskTap: true,\n    enter: 'right',\n    exit: 'right',\n    style: {\n        padding: 0\n    }\n}"
            },
            {
                "name": "width",
                "type": "Number",
                "doc": "\nThe width of the configurator panel.\n",
                "default": "400"
            }
        ],
        "events": [
            {
                "name": "applyconfigfieldsettings",
                "doc": "Fired on the pivot component after settings are applied to the configurator field.\n"
            },
            {
                "name": "beforeapplyconfigfieldsettings",
                "doc": "Fired on the pivot component before settings are applied to the configurator field.\n\nReturn false if you don't want to apply the settings to the field.\n"
            },
            {
                "name": "beforeconfigchange",
                "doc": "Fired on the pivot component before the new configuration is applied.\n\nReturn false if you don't want to apply the new configuration to the pivot grid.\n"
            },
            {
                "name": "beforemoveconfigfield",
                "doc": "Fired on the pivot component before a configurator field is moved.\n\nReturn false if you don't want to move that field.\n"
            },
            {
                "name": "beforeshowconfigfieldsettings",
                "doc": "Fired on the pivot component before the field settings container is shown.\n\nReturn false if you don't want to show the field settings container.\n"
            },
            {
                "name": "configchange",
                "doc": "Fired on the pivot component when the configuration changes.\n"
            },
            {
                "name": "hideconfigpanel",
                "doc": "Fired on the pivot component when the configurator panel is disabled\n"
            },
            {
                "name": "showconfigfieldsettings",
                "doc": "Fired on the pivot component after the configurator field settings container is shown.\n"
            },
            {
                "name": "showconfigpanel",
                "doc": "Fired on the pivot component when the configurator panel is visible\n"
            }
        ]
    },
