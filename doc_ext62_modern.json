            {
                "name": "max",
                "type": "Number",
                "doc": "The maximum length value.\n",
                "default": "undefined"
            },
            {
                "name": "maxOnlyMessage",
                "type": "String",
                "doc": "The error message to return when the value is more than the maximum\nand only a maximum is specified.\n",
                "default": "null"
            },
            {
                "name": "min",
                "type": "Number",
                "doc": "The minimum length value.\n",
                "default": "undefined"
            },
            {
                "name": "minOnlyMessage",
                "type": "String",
                "doc": "The error message to return when the value is less than the minimum\nand only a minimum is specified.\n",
                "default": "null"
            }
        ],
        "events": []
