    "Ext.calendar.EventBase": {
        "type": "Ext.calendar.EventBase",
        "doc": "A base class for an event widget. A default implementation is provided\nby {@link Ext.calendar.Event}. This class should be extended to\nprovide a custom implementation.\n",
        "configs": [
            {
                "name": "defaultTitle",
