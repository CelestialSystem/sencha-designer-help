            {
                "name": "validateeventdrop",
                "doc": "Fired when an event is dropped on this view, allows the drop\nto be validated. Depends on the {@link #cfg!droppable #droppable} config.\n"
            }
        ]
    },
    "Ext.calendar.view.WeeksRenderer": {
