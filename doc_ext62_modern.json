    "Ext.Gadget": {
        "type": "Ext.Gadget",
        "doc": "\n`{@link Ext.Gadget}` is not actually a class but simply an alternate class name for `{@link Ext.Component}`.\nIt is used primarily for writing cross-toolkit widgets.\n\nIn the classic toolkit, `{@link Ext.Gadget}` is an alternate class name for `{@link Ext.Widget}`. Since\nmost containers do not accept `{@link Ext.Widget}` as `items` in the modern toolkit, this alternate\nname allows gadgets to be used in a cross-toolkit manner.\n",
        "configs": [],
        "events": []
    },
    "Ext.GlobalEvents": {
        "type": "Ext.GlobalEvents",
        "doc": "An `{@link Ext.mixin.Observable}` through which Ext fires global events.\n\nExt.on() and Ext.un() are shorthand for {@link #method!addListener #addListener} and {@link #method!removeListener #removeListener}\non this Observable.  For example, to listen for the idle event:\n\n    Ext.on('idle', function() {\n        // do something\n    });\n",
        "configs": [],
        "events": [
            {
                "name": "added",
                "doc": "Fires when a Component is added to a Container.\n"
            },
            {
                "name": "beforeresponsiveupdate",
                "doc": "Fires before {@link Ext.mixin.Responsive} perform any updates in response to\ndynamic changes. This is prior to refreshing `responsiveFormulas`.\n"
            },
            {
                "name": "beginresponsiveupdate",
                "doc": "Fires when {@link Ext.mixin.Responsive} is about to perform updates in response to\ndynamic changes. At this point all `responsiveFormulas` have been refreshed.\n"
            },
            {
                "name": "collapse",
                "doc": "Fires when a Component is collapsed (e.g., a panel).\n"
            },
            {
                "name": "expand",
                "doc": "Fires when a Component is expanded (e.g., a panel).\n"
            },
            {
                "name": "hide",
                "doc": "Fires when a Component is hidden.\n"
            },
            {
                "name": "idle",
                "doc": "Fires when an event handler finishes its run, just before returning to\nbrowser control.\n\nThis includes DOM event handlers, Ajax (including JSONP) event handlers,\nand {@link Ext.util.TaskRunner}\n\nWhen called at the tail of a DOM event, the event object is passed as the\nsole parameter.\n\nThis can be useful for performing cleanup, or update tasks which need to\nhappen only after all code in an event handler has been run, but which\nshould not be executed in a timer due to the intervening browser\nreflow/repaint which would take place.\n"
            },
            {
                "name": "mousedown",
                "doc": "A mousedown listener on the document that is immune to stopPropagation()\nused in cases where we need to know if a mousedown event occurred on the\ndocument regardless of whether some other handler tried to stop it.  An\nexample where this is useful is a menu that needs to be hidden whenever\nthere is a mousedown event on the document.\n"
            },
            {
                "name": "onlinechange",
                "doc": "Fires when the online status of the page changes. See {@link Ext#method-isOnline}\n"
            },
            {
                "name": "removed",
                "doc": "Fires when a Component is removed from a Container.\n"
            },
            {
                "name": "resize",
                "doc": "Fires when the browser window is resized.  To avoid running resize handlers\ntoo often resulting in sluggish window resizing, resize events use a buffer\nof 100 milliseconds.\n"
            },
            {
                "name": "responsiveupdate",
                "doc": "Fires after {@link Ext.mixin.Responsive} has performed updates in response to\ndynamic changes.\n"
            },
            {
                "name": "show",
                "doc": "Fires when a Component is shown.\n"
            }
        ]
    },
