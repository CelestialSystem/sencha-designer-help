    "Ext.drag.Constraint": {
        "type": "Ext.drag.Constraint",
        "doc": "Provides constraining behavior for a {@link Ext.drag.Source}.\n",
        "configs": [
            {
                "name": "element",
                "type": "Boolean/String/HTMLELement/Ext.dom.Element",
                "doc": "\nThe element to constrain to:\n- `true` to constrain to the parent of the {@link Ext.drag.Source#cfg!element Ext.drag.Source#element}.\n- The id, DOM element or Ext.dom.Element to constrain to.\n",
                "default": "null"
            },
            {
                "name": "horizontal",
                "type": "Boolean",
                "doc": "`true` to limit dragging to the horizontal axis.\n",
                "default": "false"
            },
            {
                "name": "region",
                "type": "Ext.util.Region",
                "doc": "\nThe region to constrain to.\n",
                "default": "null"
            },
            {
                "name": "snap",
                "type": "Number/Object",
                "doc": "The interval to move this drag target during a drag in both dimensions.\n- `{x: 30}`, snap only x\n- `{y: 30}`, snap only y\n- `{x: 30, y: 40}`, snap both\n- `40`, snap both to `40`.\n\nThe snap may also be a function to calculate the snap value on each tick.\n\n     snap: {\n         x: function(info, x) {\n             return x < 300 ? 150 : 500;\n         }\n     }\n",
                "default": "null"
            },
            {
                "name": "source",
                "type": "Ext.drag.Source",
                "doc": "The {@link Ext.drag.Source} for the constraint. This will be\nset automatically when constructed via the source.\n",
                "default": "null"
            },
            {
                "name": "vertical",
                "type": "Boolean",
                "doc": "`true` to limit dragging to the vertical axis.\n",
                "default": "false"
            },
            {
                "name": "x",
                "type": "Number[]",
                "doc": "The minimum and maximum x position. Use `null` to\nnot set a constraint:\n- `[100, null]`, constrain only the minimum\n- `[null, 100]`, constrain only the maximum\n- `[200, 200]`, constrain both.\n",
                "default": "null"
            },
            {
                "name": "y",
                "type": "Number[]",
                "doc": "The minimum and maximum y position. Use `null` to\nnot set a constraint:\n- `[100, null]`, constrain only the minimum\n- `[null, 100]`, constrain only the maximum\n- `[200, 200]`, constrain both.\n",
                "default": "null"
            }
        ],
        "events": []
    },
