    "Ext.data.proxy.Client": {
        "type": "Ext.data.proxy.Client",
        "doc": "Base class for any client-side storage. Used as a superclass for {@link Ext.data.proxy.Memory} and\n{@link Ext.data.proxy.WebStorage} proxies. Do not use directly, use one of the subclasses instead.\n",
        "configs": [],
        "events": []
