    "Ext.draw.sprite.Arc": {
        "type": "Ext.draw.sprite.Arc",
        "doc": "\n A sprite that represents a circular arc.\n\n    @example\n    Ext.create({\n       xtype: 'draw',\n       renderTo: document.body,\n       width: 600,\n       height: 400,\n       sprites: [{\n           type: 'arc',\n           cx: 100,\n           cy: 100,\n           r: 80,\n           fillStyle: '#1F6D91',\n           startAngle: 0,\n           endAngle: Math.PI,\n           anticlockwise: true\n       }]\n    });\n",
