    "Ext.chart.grid.HorizontalGrid3D": {
        "type": "Ext.chart.grid.HorizontalGrid3D",
        "doc": "\nHorizontal 3D Grid sprite. Used in 3D Cartesian Charts.\n",
        "configs": [],
        "events": []
