            {
                "name": "modal",
                "type": "Boolean",
                "doc": "`true` to make this Componenrt modal. This will create a mask underneath the Component\nthat covers its parent and does not allow the user to interact with any other Components until this\nComponent is dismissed.\n",
                "default": "true"
            },
            {
                "name": "showAnimation",
                "type": "String/Mixed",
                "doc": "Animation effect to apply when the Component is being shown.  Typically you want to use an\ninbound animation type such as 'fadeIn' or 'slideIn'. For more animations, check the {@link Ext.fx.Animation#cfg!type Ext.fx.Animation#type} config.\n",
                "default": "{\n    type: 'slideIn',\n    duration: 250,\n    easing: 'ease-out'\n}"
            },
            {
                "name": "stretchX",
                "type": "Boolean",
                "doc": "`true` to stretch this sheet horizontally.\n",
                "default": "null"
            },
            {
                "name": "stretchY",
                "type": "Boolean",
                "doc": "`true` to stretch this sheet vertically.\n",
                "default": "null"
            }
        ],
        "events": []
    },
    "Ext.slider.Slider": {
        "type": "Ext.slider.Slider",
        "doc": "Slider component used by Ext.field.Slider.\n",
        "configs": [
            {
                "name": "allowThumbsOverlapping",
                "type": "Boolean",
                "doc": "Whether or not to allow multiple thumbs to overlap each other.\nSetting this to true guarantees the ability to select every possible value in between {@link #cfg!minValue #minValue}\nand {@link #cfg!maxValue #maxValue} that satisfies {@link #cfg!increment #increment}\n",
                "default": "false"
            },
            {
                "name": "animation",
                "type": "Boolean/Object",
                "doc": "The animation to use when moving the slider. Possible properties are:\n\n- duration\n- easingX\n- easingY\n",
                "default": "true"
            },
            {
                "name": "increment",
                "type": "Number",
                "doc": "The increment by which to snap each thumb when its value changes. Any thumb movement\nwill be snapped to the nearest value that is a multiple of the increment (e.g. if increment is 10 and the user\ntries to move the thumb to 67, it will be snapped to 70 instead)\n",
