    "Ext.exporter.excel.Xml": {
        "type": "Ext.exporter.excel.Xml",
        "doc": "This exporter produces Microsoft Excel 2003 XML files for the supplied data. It was implemented according to\n[this][1] documentation.\n\n[1]: https://msdn.microsoft.com/en-us/Library/aa140066(v=office.10).aspx\n",
        "configs": [
            {
                "name": "defaultStyle",
