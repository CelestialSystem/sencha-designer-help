                "type": "Object",
                "doc": "The configuration for the sheet in {@link #compact} mode.\n",
                "default": "{\n    xtype: 'sheet',\n    reference: 'sheet',\n    cls: Ext.baseCSSPrefix + 'calendar-panel-sheet',\n    centered: false,\n    enter: 'left',\n    exit: 'left',\n    hideOnMaskTap: true,\n    stretchY: true,\n    ui: 'light',\n    header: {\n        border: false,\n        title: 'Calendars'\n    }\n}"
            }
