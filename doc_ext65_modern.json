    "Ext.direct.RemotingEvent": {
        "type": "Ext.direct.RemotingEvent",
        "doc": "An event that is fired when data is received from a\n{@link Ext.direct.RemotingProvider}. Contains a method to the\nrelated transaction for the direct request, see {@link #getTransaction}\n",
