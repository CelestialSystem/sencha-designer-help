    "Ext.chart.Caption": {
        "type": "Ext.chart.Caption",
        "doc": "Chart captions can be used to place titles, subtitles, credits and other captions\ninside a chart. Please see the chart's {@link Ext.chart.AbstractChart#captions}\nconfig documentation for the general description of the way captions work, and\nrefer to the documentation of this class' configs for details.\n",
        "configs": [
            {
                "name": "align",
                "type": "'left'/'center'/'right'",
                "doc": "Determines the horizontal alignment of the caption's text.\n",
                "default": "'center'"
            },
            {
                "name": "alignTo",
                "type": "'series'/'chart'",
                "doc": "Whether to align the caption to the 'series' (default) or the 'chart'.\n",
                "default": "'series'"
            },
            {
                "name": "docked",
                "type": "'top'/'bottom'",
                "doc": "The position of the caption in a chart.\n",
                "default": "'top'"
            },
            {
                "name": "hidden",
                "type": "Boolean",
                "doc": "Controls the visibility of the caption.\n",
                "default": "false"
            },
            {
                "name": "padding",
                "type": "Number",
                "doc": "The uniform padding applied to both top and bottom of the caption's text.\n",
                "default": "0"
            },
            {
                "name": "style",
                "type": "Object",
                "doc": "Style attributes for the caption's text.\nAll attributes that are valid {@link Ext.draw.sprite.Text text sprite} attributes\nare valid here. However, only font attributes (such as `fontSize`, `fontFamily`, ...),\ncolor attributes (such as `fillStyle`) and `textAlign` attribute are guaranteed to\nproduce correct behavior. For example, transform attributes are not officially supported.\n",
                "default": "{\n    fontSize: '14px',\n    fontWeight: 'bold',\n    fontFamily: 'Verdana, Aria, sans-serif'\n}"
            },
            {
                "name": "text",
                "type": "String",
                "doc": "The text displayed by the caption.\nMulti-line captions are allowed, e.g.:\n\n    captions: {\n        title: {\n            text: 'India\\'s tiger population\\n'\n                + 'from 1970 to 2015'\n        }\n    }\n",
                "default": "''"
            },
            {
                "name": "weight",
                "doc": "The weight controls the order in which the captions are created.\nCaptions with lower weights are created first.\nThis affects chart's layout. For example, if two captions are docked\nto the 'top', the one with the lower weight will end up on top\nof the other.\n",
                "default": "0"
            }
        ],
        "events": []
    },
    "Ext.chart.CartesianChart": {
        "type": "Ext.chart.CartesianChart",
        "doc": "Represents a chart that uses cartesian coordinates.\nA cartesian chart has two directions, X direction and Y direction.\nThe series and axes are coordinated along these directions.\nBy default the x direction is horizontal and y direction is vertical,\nYou can swap the direction by setting the {@link #flipXY} config to `true`.\n\nCartesian series often treats x direction an y direction differently.\nIn most cases, data on x direction are assumed to be monotonically increasing.\nBased on this property, cartesian series can be trimmed and summarized properly\nto gain a better performance.\n",
        "configs": [
            {
                "name": "flipXY",
                "type": "Boolean",
                "doc": "Flip the direction of X and Y axis.\nIf flipXY is `true`, the X axes will be vertical and Y axes will be horizontal.\nNote that {@link Ext.chart.axis.Axis#position positions} of chart axes have\nto be updated accordingly: axes positioned to the `{@link #cfg!top top}` and `{@link #cfg!bottom bottom}` should\nbe positioned to the `{@link #cfg!left left}` or `{@link #cfg!right right}` and vice versa.\n",
                "default": "false"
            },
            {
                "name": "innerPadding",
                "type": "Object",
                "doc": "The amount of inner padding in pixels.\nInner padding is the padding from the innermost axes to the series.\n",
                "default": "{\n    top: 0,\n    left: 0,\n    right: 0,\n    bottom: 0\n}"
            }
        ],
        "events": []
    },
