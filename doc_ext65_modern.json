    "Ext.d3.mixin.ToolTip": {
        "type": "Ext.d3.mixin.ToolTip",
        "doc": "The ToolTip mixin is used to add {@link Ext.tip.ToolTip tooltip} support to various\nD3 components.\n",
        "configs": [
            {
                "name": "tooltip",
                "type": "Ext.tip.ToolTip",
                "doc": "The {@link Ext.tip.ToolTip} class config object with one extra supported\nproperty: `renderer`.\n",
