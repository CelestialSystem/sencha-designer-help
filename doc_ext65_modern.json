    "Ext.panel.Resizer": {
        "type": "Ext.panel.Resizer",
        "doc": "This class allows a {@link Ext.Panel Panel} to be resized via user interaction.\nIt can be used on floating panels, or as a splitter between two panels.\n",
        "configs": [
            {
                "name": "constrainToParent",
                "type": "Boolean",
                "doc": "`true` to constrain the dragging operation to the parent\nof the {@link #cfg-target}.\n",
                "default": "true"
            },
            {
                "name": "dynamic",
                "type": "Boolean",
                "doc": "`true` to live resize the {@link #cfg-target}. `false` to create\na proxy indicator to represent the drag operation.\n",
                "default": "false"
            },
            {
                "name": "edges",
                "type": "String/String[]",
                "doc": "The draggable edges. These can be specified as a string separated by ' ' or ','. The\nvalues for the edges should be direction coordinates (or the shortcut). Possible values are:\n\n- `'north'`, or `'n'`\n- `'northeast'`, or `'ne'`\n- `'east'`, or `'e'`\n- `'southeast'`, or `'se'`\n- `'south'`, or `'s'`\n- `'southwest'`, or `'sw'`\n- `'west'`, or `'w'`\n- `'northwest'`, or `'nw'`\n- `'all'`, a shortcut for all edges\n\nExamples:\n- `['n', 'e', 's', 'w']`,\n- `'e,se,s'`\n- `'e se s'`,\n- `'northeast southeast southwest northwest'`\n",
                "default": "null"
            },
            {
                "name": "maxSize",
                "type": "Number/Number[]",
                "doc": "The maximum width and height for this resizer. If specified as a number,\nthe value applies for both width and height. Otherwise,\n- `[100, null]`, constrain only the width\n- `[null, 100]`, constrain only the height\n- `[200, 300]`, constrain both.\n\n**Note** If a {@link Ext.Component#cfg-maxWidth maxWidth} or {@link Ext.Component#cfg-maxHeight maxHeight}\nis specified, it will take precedence.\n",
                "default": "null"
            },
            {
                "name": "minSize",
                "type": "Number/Number[]",
                "doc": "The minimum width and height for this resizer. If specified as a number,\nthe value applies for both width and height. Otherwise,\n- `[100, null]`, constrain only the width\n- `[null, 100]`, constrain only the height\n- `[200, 300]`, constrain both.\n\n**Note** If a {@link Ext.Component#cfg-minWidth minWidth} or {@link Ext.Component#cfg-minHeight minHeight}\nis specified, it will take precedence.\n",
                "default": "null"
            },
            {
                "name": "preserveRatio",
                "doc": "{@cfg} {Boolean} preserveRatio\n`true` to preserve the current aspect ratio of the component while dragging.\n",
                "default": "false"
            },
            {
                "name": "snap",
                "type": "Number/Number[]",
                "doc": "The interval to move during a resize. If specified as a number,\nthe value applies for both width and height. Otherwise,\n- `[100, null]`, snap only the width\n- `[null, 100]`, snap only the height\n- `[200, 300]`, snap both.\n",
                "default": "null"
            },
            {
                "name": "split",
                "type": "Boolean",
                "doc": "`true` to operate in splitter mode, which is typically used for {@link Ext.Component#cfg-docked docked} items or\nitems in a {@link Ext.layout.Box box layout}. `false` to operate in handle mode, which is often used for floating\nitems.\n",
                "default": "false"
            },
            {
                "name": "target",
                "type": "Ext.Panel",
                "doc": "The panel to be resized.\n",
                "default": "null"
            }
        ],
        "events": []
    },
