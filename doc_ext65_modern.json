    "Ext.sparkline.Line": {
        "type": "Ext.sparkline.Line",
        "doc": "\nPlots a line graph based upon the input {@link #values} array.\n\nSee {@link Ext.sparkline.Base the base class} for a simple example.\n",
        "configs": [
            {
                "name": "chartRangeMax",
