    "Ext.layout.Auto": {
        "type": "Ext.layout.Auto",
        "configs": [
            {
                "name": "animation",
                "type": "Ext.layout.card.fx.Abstract",
                "doc": "Layout animation configuration\nControls how layout transitions are animated.  Currently only available for\nCard Layouts.\n\nPossible values are:\n\n- cover\n- cube\n- fade\n- flip\n- pop\n- reveal\n- scroll\n- slide\n",
                "default": "null"
            },
            {
                "name": "container",
                "type": "Ext.Container",
                "doc": "The container that this layout manages\n",
                "default": "null"
            }
        ],
