            },
            {
                "name": "colorMap",
                "type": "Object",
                "doc": "\nPlots bars based upon \"win\"/\"draw\" or \"lose\" status of the input {@link #values} array. Positive values mean\na win, zero a draw, and negative a lose.\n\nSee {@link Ext.sparkline.Base the base class} for a simple example.\n",
                "default": "null"
