            {
                "name": "DOMNodeRemovedFromDocument",
                "doc": "Where supported. Fires when a node is being removed from a document.\n"
            },
            {
                "name": "DOMSubtreeModified",
                "doc": "Where supported. Fires when the subtree is modified.\n"
            },
            {
                "name": "doubletap",
                "doc": "Fires when there is a double tap.\n"
            },
            {
                "name": "error",
                "doc": "Fires when an object/image/frame cannot be loaded properly.\n"
            },
            {
                "name": "focus",
                "doc": "Fires when an element receives focus either via the pointing device or by tab navigation.\n"
            },
            {
                "name": "focusmove",
                "doc": "Fires when focus is moved *within* an element.\n"
            },
            {
                "name": "keydown",
                "doc": "Fires when a keydown is detected within the element.\n"
            },
            {
                "name": "keypress",
                "doc": "Fires when a keypress is detected within the element.\n"
            },
            {
                "name": "keyup",
                "doc": "Fires when a keyup is detected within the element.\n"
            },
            {
                "name": "load",
                "doc": "Fires when the user agent finishes loading all content within the element. Only supported by window, frames,\nobjects and images.\n"
            },
            {
                "name": "longpress",
                "doc": "Fires when you touch and hold still for more than 1 second.\n"
            },
            {
                "name": "mousedown",
                "doc": "Fires when a mousedown is detected within the element.\n"
            },
            {
                "name": "mouseenter",
                "doc": "Fires when the mouse enters the element.\n"
            },
            {
                "name": "mouseleave",
                "doc": "Fires when the mouse leaves the element.\n"
            },
            {
                "name": "mousemove",
                "doc": "Fires when a mousemove is detected with the element.\n"
            },
            {
                "name": "mouseout",
                "doc": "Fires when a mouseout is detected with the element.\n"
            },
            {
                "name": "mouseover",
                "doc": "Fires when a mouseover is detected within the element.\n"
            },
            {
                "name": "mouseup",
                "doc": "Fires when a mouseup is detected within the element.\n"
            },
            {
                "name": "painted",
                "doc": "Fires whenever this Element actually becomes visible (painted) on the screen. This is useful when you need to\nperform 'read' operations on the DOM element, i.e: calculating natural sizes and positioning.\n\n__Note:__ This event is not available to be used with event delegation. Instead `{@link #event!painted painted}` only fires if you explicitly\nadd at least one listener to it, for performance reasons.\n"
            },
            {
                "name": "pinch",
                "doc": "Fires continuously when there is pinching (the touch must move for this to be fired).\n"
            },
