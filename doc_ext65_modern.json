    "Ext.dataview.pullrefresh.Item": {
        "type": "Ext.dataview.pullrefresh.Item",
        "doc": "This component is the base class for {@link Ext.dataview.pullrefresh.Bar pullrefreshbar}\nand {@link Ext.dataview.pullrefresh.Spinner pullrefreshspinner}.\n",
        "configs": [],
        "events": []
    },
    "Ext.dataview.pullrefresh.PullRefresh": {
        "type": "Ext.dataview.pullrefresh.PullRefresh",
        "doc": "This {@link Ext.Component#cfg!plugins plugin} adds pull to refresh functionality to the\n{@link Ext.dataview.List list} component.\n\n## Example\n\n     @example\n     Ext.create({\n         xtype: 'list',\n         fullscreen: true,\n\n         plugins: {\n             pullrefresh: {\n                 pullText: 'Pull down for more new Tweets!'\n             }\n         },\n\n         itemTpl: [\n             '<img src=\"{img}\" alt=\"{name} photo\" />',\n             '<div class=\"tweet\"><b>{name}:</b> {text}</div>'\n         ],\n\n         store: [{\n             name: 'Bill',\n             img: 'https://www.sencha.com/forum/images/statusicon/forum_new-48.png',\n             text: 'JavaScript development'\n         }]\n     });\n",
        "configs": [
            {
                "name": "autoSnapBack",
                "type": "Boolean",
                "doc": "Determines whether the pulldown should automatically snap back after data has\nbeen loaded. If `false` call {@link #snapBack} to manually snap the pulldown back.\n",
                "default": "true"
            },
            {
                "name": "lastUpdatedDateFormat",
                "type": "String",
                "doc": "The format to be used on the last updated date.\n"
            },
            {
                "name": "lastUpdatedText",
                "type": "String",
                "doc": "The text to be shown in front of the last updated time.\n"
            },
            {
                "name": "loadedText",
                "type": "String",
                "doc": "The text that will be when data has been loaded.\n"
            },
            {
                "name": "loadingText",
                "type": "String",
                "doc": "The text that will be shown while the list is refreshing.\n"
            },
            {
                "name": "mergeData",
                "type": "Boolean",
                "doc": "`true` to insert new records into the store and to replace the data for\nany incoming records that exist.\n\n`false` to completely overwrite store data with the fetched response.\n",
                "default": "true"
            },
            {
                "name": "overlay",
                "type": "Boolean",
                "doc": "`false` to move the list down to display the refresh indicator. `true` to float\nthe indicator over the top of the list with no movement.\n",
                "default": "false"
            },
            {
                "name": "pullText",
                "type": "String",
                "doc": "The text that will be shown while you are pulling down.\n"
            },
            {
                "name": "releaseText",
                "type": "String",
                "doc": "The text that will be shown after you have pulled down enough to show the\nrelease message.\n"
            },
            {
                "name": "snappingAnimationDuration",
                "type": "Number",
                "doc": "The duration for snapping back animation after the data has been refreshed\n",
                "default": "300"
            }
        ],
        "events": []
    },
    "Ext.dataview.pullrefresh.Spinner": {
