    "Ext.tab.Bar": {
        "type": "Ext.tab.Bar",
        "doc": "Ext.tab.Bar is used internally by {@link Ext.tab.Panel} to create the bar of tabs that appears at the top of the tab\npanel. It can also be used as a standalone component to recreate the look and feel of tabs.\n",
        "configs": [
            {
                "name": "activeTab",
                "type": "Number/String/Ext.Component",
                "doc": "The initially activated tab. Can be specified as numeric index, itemId,\ncomponent ID, or as the component instance itself.\n",
                "default": "null"
            },
            {
                "name": "animateIndicator",
                "type": "Boolean",
                "doc": "Determines if the active indicator below the tab should animate or snap\n",
                "default": "false"
            },
            {
                "name": "defaultTabUI",
                "type": "String",
                "doc": "A default {@link Ext.Component#ui ui} to use for {@link Ext.tab.Tab Tab} items.\n",
                "default": "null"
            },
