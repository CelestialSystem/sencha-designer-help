    "Ext.mixin.FocusableContainer": {
        "type": "Ext.mixin.FocusableContainer",
        "doc": "A mixin for groups of Focusable things (Components, Widgets, etc) that\nshould respond to arrow keys to navigate among the peers, but keep only\none of the peers tabbable by default (tabIndex=0)\n\nSome examples: Toolbars, Tab bars, Panel headers, Menus\n",
        "configs": [
            {
                "name": "activeChildTabIndex",
                "type": "Number",
                "doc": "DOM tabIndex attribute to set on the\nactive Focusable child of this container when using the \"Roaming tabindex\"\ntechnique.\n",
                "default": "0"
            },
            {
                "name": "allowFocusingDisabledChildren",
                "type": "Boolean",
                "doc": "Set this to `true`\nto enable focusing disabled child items via keyboard.\n",
                "default": "false"
            },
            {
                "name": "focusableContainer",
                "type": "Boolean",
                "doc": "Enable or disable navigation\nwith arrow keys for this FocusableContainer. This option may be useful\nwith nested FocusableContainers, when only the root container should\nhandle keyboard events.\n",
                "default": "false"
            },
            {
                "name": "inactiveChildTabIndex",
                "type": "Number",
                "doc": "DOM tabIndex attribute to set on\ninactive Focusable children of this container when using the \"Roaming tabindex\"\ntechnique. This value rarely needs to be changed from its default.\n",
                "default": "-1"
            },
            {
                "name": "resetFocusPosition",
                "type": "Boolean",
                "doc": "When `true`, FocusableContainer\nwill reset last focused position whenever focus leaves the container.\nSubsequent tabbing into the container will always focus the first eligible\nchild item.\n\nWhen `false`, subsequent tabbing into the container will focus the child\nitem that was last focused before.\n",
                "default": "false"
            }
        ],
        "events": []
    },
