    "Ext.data.validator.Validator": {
        "type": "Ext.data.validator.Validator",
        "doc": "The base class for validators to be used to validate {@link Ext.data.Field fields} in\na {@link Ext.data.Model model}.\n\nThe model will call the {@link #validate} method, which may be overridden by subclasses.\n",
        "configs": [],
        "events": []
