    "Ext.Progress": {
        "type": "Ext.Progress",
        "doc": "\n    @example\n    Ext.create({\n        xtype: 'grid',\n        title: 'Simpsons',\n        store: {\n            data: [\n                { name: 'Lisa', progress: .159 },\n                { name: 'Bart', progress: .216 },\n                { name: 'Homer', progress: .55 },\n                { name: 'Maggie', progress: .167 },\n                { name: 'Marge', progress: .145 }\n            ]\n        },\n        columns: [\n            { text: 'Name',  dataIndex: 'name' },\n            {\n                text: 'Progress',\n                width: 120,\n                dataIndex: 'progress',\n                cell: {\n                    xtype: 'widgetcell',\n                    widget: {\n                        xtype: 'progress'\n                    }\n                }\n            }\n        ],\n        height: 200,\n        width: 400,\n        fullscreen: true\n    });\n",
        "configs": [
