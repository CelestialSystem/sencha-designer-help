    "Ext.dataview.listswiper.ListSwiper": {
        "type": "Ext.dataview.listswiper.ListSwiper",
        "doc": "This plugin provides a way to map actions to swipe gesture on all list items.\n",
        "configs": [
            {
                "name": "commitDelay",
                "type": "Number",
                "doc": "Number of milliseconds before actions in the undo state are automatically committed (`0` to\ndisable this behavior). Only applicable for {@link #actions} with `undoable: true`.\n",
                "default": "0"
            },
            {
                "name": "dismissOnScroll",
                "type": "Boolean",
                "doc": "If `true`, actions in the undo state will be committed as soon as the list is scrolled.\nAny open menus will be closed\n",
                "default": "true"
            },
            {
                "name": "dismissOnTap",
                "type": "Boolean",
                "doc": "If `true`, actions in the undo state will be committed when the item is tapped.\nAny open menus will be closed.\n",
                "default": "true"
            },
            {
                "name": "target",
                "type": "'inner'/'outer'",
                "doc": "The section of the list item that is swipable.  Supports the following values:\n\n- `'inner'` - the default value. the body of the list item, which includes any\ntools is swipable, and any docked items remain fixed in place while swiping.\n- `'outer'` - the entire list item including the docked items is swipable\n",
                "default": "'inner'"
            },
            {
                "name": "widget",
                "type": "Object/Ext.dataview.plugin.ListSwiperStepper",
                "doc": "The config object for a {@link Ext.dataview.plugin.ListSwiperStepper}.\n",
                "default": "{\n    xtype: 'listswiperaccordion'\n}"
            }
        ],
        "events": []
    },
    "Ext.dataview.listswiper.Stepper": {
        "type": "Ext.dataview.listswiper.Stepper",
        "doc": "This class is a container used by the {@link Ext.dataview.plugin.ListSwiper listswiper}\nplugin to display information and controls when an item is swiped.\n",
        "configs": [
            {
                "name": "animation",
                "type": "Boolean/Object",
                "doc": "`true` for the default animation (`{ duration: 500, easing: 'ease-out' }`) or\na standard animation config object to be used for default swipe animations.\n",
                "default": "true"
            },
            {
                "name": "iconCls",
                "type": "String",
                "doc": "One or more space separated CSS classes to be applied to the icon element.\nSee {@link Ext.Button#iconCls} for details.\n",
                "default": "null"
            },
            {
                "name": "text",
                "type": "String",
                "doc": "The swipe action text.\n",
                "default": "null"
            },
            {
                "name": "undo",
                "type": "Object",
                "doc": "A config object for the undo button.\n",
                "default": "{\n    docked: 'right',\n    ui: 'listswiperstepper-trigger'\n}"
            }
        ],
        "events": []
    },
    "Ext.dataview.Location": {
        "type": "Ext.dataview.Location",
        "doc": "Instances of this class encapsulate a focusable item in a DataView.\n\nDataView addresses are configured using the owning {@link #property!item}.\n\nBe careful not to make `Location` objects persistent. If the associated record\nis removed or filtered out, or is scrolled out of the rendered range in an infinite\nlist, the reference will be stale. Its referenced item may be used by a different record.\n\nFreshly created Location objects, such as those exposed by events from the\n{@link Ext.dataview.selection.Model selection model} are safe to use until your\napplication mutates the store, changes the column set (for grids), or scrolls\nthe referenced item out of the rendered block (for infinite lists).\n",
        "configs": [],
        "events": []
    },
    "Ext.dataview.NavigationModel": {
        "type": "Ext.dataview.NavigationModel",
        "doc": "This class listens for events fired from a {@link Ext.dataview.DataView DataView},\nand tracks the currently focused item.\n",
        "configs": [],
        "events": []
    },
    "Ext.dataview.NestedList": {
        "type": "Ext.dataview.NestedList",
        "doc": "NestedList provides a miller column interface to navigate between nested sets\nand provide a clean interface with limited screen real-estate.\n\n    @example\n     var data = {\n         text: 'Groceries',\n         items: [{\n             text: 'Drinks',\n             items: [{\n                 text: 'Water',\n                 items: [{\n                     text: 'Sparkling',\n                     leaf: true\n                 }, {\n                     text: 'Still',\n                     leaf: true\n                 }]\n             }, {\n                 text: 'Coffee',\n                 leaf: true\n             }, {\n                 text: 'Espresso',\n                 leaf: true\n             }, {\n                 text: 'Redbull',\n                 leaf: true\n             }, {\n                 text: 'Coke',\n                 leaf: true\n             }, {\n                 text: 'Diet Coke',\n                 leaf: true\n             }]\n         }, {\n             text: 'Fruit',\n             items: [{\n                 text: 'Bananas',\n                 leaf: true\n             }, {\n                 text: 'Lemon',\n                 leaf: true\n             }]\n         }, {\n             text: 'Snacks',\n             items: [{\n                 text: 'Nuts',\n                 leaf: true\n             }, {\n                 text: 'Pretzels',\n                 leaf: true\n             }, {\n                 text: 'Wasabi Peas',\n                 leaf: true\n             }]\n         }]\n     };\n\n     Ext.define('ListItem', {\n         extend: 'Ext.data.Model',\n         config: {\n             fields: [{\n                 name: 'text',\n                 type: 'string'\n             }]\n         }\n     });\n\n     var store = Ext.create('Ext.data.TreeStore', {\n         model: 'ListItem',\n         defaultRootProperty: 'items',\n         root: data\n     });\n\n     var nestedList = Ext.create('Ext.NestedList', {\n         fullscreen: true,\n         title: 'Groceries',\n         displayField: 'text',\n         store: store\n     });\n",
        "configs": [
            {
                "name": "allowDeselect",
                "type": "Boolean",
                "doc": "Set to `true` to allow the user to deselect leaf items via interaction.\n",
                "default": "false"
            },
            {
                "name": "backButton",
                "type": "Object",
                "doc": "The configuration for the back button used in the nested list.\n",
                "default": "{\n    hidden: true\n}"
            },
            {
                "name": "backText",
                "type": "String",
                "doc": "The label to display for the back button.\n",
                "default": "'Back'"
            },
            {
                "name": "cardSwitchAnimation",
                "type": "String/Object/Boolean",
                "doc": "Animation to be used during transitions of cards.\n"
            },
            {
                "name": "detailCard",
                "type": "Ext.Component",
                "doc": "provides the information for a leaf\nin a Miller column list. In a Miller column, users follow a\nhierarchial tree structure to a leaf, which provides information\nabout the item in the list. The detailCard lists the information at\nthe leaf.\n\nSee http://en.wikipedia.org/wiki/Miller_columns\n",
                "default": "null"
            },
            {
                "name": "detailContainer",
                "type": "Ext.Container",
                "doc": "The container of the `{@link #cfg!detailCard detailCard}`.\nA detailContainer is a reference to the container where a detail card\ndisplays.\n\nSee http://en.wikipedia.org/wiki/Miller_columns\n\nThe two possible values for a detailContainer are undefined (default),\nwhich indicates that a detailCard appear in the same container, or you\ncan specify a new container location. The default condition uses the\ncurrent List container.\n\nThe following example shows creating a location for a detailContainer:\n\nvar detailContainer = Ext.create('Ext.Container', {\n    layout: 'card'\n});\n\nvar nestedList = Ext.create('Ext.NestedList', {\n    store: treeStore,\n    detailCard: true,\n    detailContainer: detailContainer\n});\n\nThe default value is typically used for phone devices in portrait mode\nwhere the small screen size dictates that the detailCard replace the\ncurrent container.\n",
                "default": "undefined"
            },
            {
                "name": "displayField",
                "type": "String",
                "doc": "Display field to use when setting item text and title.\nThis configuration is ignored when overriding {@link #getItemTextTpl} or\n{@link #getTitleTextTpl} for the item text or title.\n",
                "default": "'text'"
            },
            {
                "name": "emptyText",
                "type": "String",
                "doc": "Empty text to display when a subtree is empty.\n",
                "default": "'No items available.'"
            },
            {
                "name": "layout",
                "type": "String",
                "default": "{\n    type: 'card',\n    animation: {\n        type: 'slide',\n        duration: 250,\n        direction: 'left'\n    }\n}"
            },
            {
                "name": "listConfig",
                "type": "Object",
                "doc": "An optional config object which is merged with the default\nconfiguration used to create each nested list.\n",
                "default": "null"
            },
            {
                "name": "loadingText",
                "type": "String",
                "doc": "Loading text to display when a subtree is loading.\n",
                "default": "'Loading...'"
            },
            {
                "name": "onItemDisclosure",
                "type": "Boolean/Function",
                "doc": "Maps to the {@link Ext.List#onItemDisclosure} configuration for individual lists.\n",
                "default": "false"
            },
            {
                "name": "store",
                "type": "Ext.data.TreeStore/String",
                "doc": "The tree store to be used for this nested list.\n",
                "default": "null"
            },
            {
                "name": "title",
                "type": "String",
                "doc": "The title of the toolbar\n",
                "default": "''"
            },
            {
                "name": "toolbar",
                "type": "Ext.Toolbar/Object/Boolean",
                "doc": "The configuration to be used for the toolbar displayed in this nested list.\n",
                "default": "{\n    docked: 'top',\n    xtype: 'titlebar',\n    ui: 'light',\n    inline: true\n}"
            },
            {
                "name": "updateTitleText",
                "type": "Boolean",
                "doc": "Update the title text with the currently selected category.\n",
                "default": "true"
            },
            {
                "name": "useTitleAsBackText",
                "type": "Boolean",
                "doc": "`true` to use title as a label for back button.\n",
                "default": "true"
            },
            {
                "name": "useToolbar",
                "type": "Boolean",
                "doc": "`true` to show the header toolbar.\n",
                "default": "null"
            },
            {
                "name": "variableHeights",
                "type": "Boolean",
                "doc": "This configuration allows you optimize the picker by not having it read the DOM heights of list items.\n",
                "default": "false"
            }
        ],
        "events": [
            {
                "name": "back",
                "doc": "Fires when the user taps Back.\n"
            },
            {
                "name": "beforeload",
                "doc": "Fires before a request is made for a new data object.\n"
            },
            {
                "name": "beforeselectionchange",
                "doc": "Fires before a selection is made.\n"
            },
            {
                "name": "containertap",
                "doc": "Fires when a tap occurs and it is not on a template node.\n"
            },
            {
                "name": "itemdoubletap",
                "doc": "Fires when a node is double tapped on.\n"
            },
            {
                "name": "itemtap",
                "doc": "Fires when a node is tapped on.\n"
            },
            {
                "name": "leafitemtap",
                "doc": "Fires when the user taps a leaf list item.\n"
            },
            {
                "name": "listchange",
                "doc": "Fires when the user taps a list item.\n"
            },
            {
                "name": "load",
                "doc": "Fires whenever records have been loaded into the store.\n"
            },
            {
                "name": "selectionchange",
                "doc": "Fires when the selected nodes change.\n"
            }
        ]
    },
