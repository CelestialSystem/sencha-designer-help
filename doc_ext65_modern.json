    "Ext.draw.sprite.Triangle": {
        "type": "Ext.draw.sprite.Triangle",
        "doc": "A sprite that represents a triangle.\n\n    @example\n    Ext.create({\n       xtype: 'draw',\n       renderTo: document.body,\n       width: 600,\n       height: 400,\n       sprites: [{\n           type: 'triangle',\n           size: 50,\n           translationX: 100,\n           translationY: 100,\n           fillStyle: '#1F6D91'\n       }]\n    });\n",
        "configs": [
            {
