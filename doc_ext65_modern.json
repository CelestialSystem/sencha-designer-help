    "Ext.calendar.EventBase": {
        "type": "Ext.calendar.EventBase",
        "doc": "A base class for an event widget. A default implementation is provided\nby {@link Ext.calendar.Event}. This class should be extended to\nprovide a custom implementation.\n",
        "configs": [
            {
                "name": "defaultTitle",
                "type": "String",
                "doc": "The default title to use when one is not specified.\n",
                "default": "'(New Event)'"
            },
            {
                "name": "endDate",
                "type": "Date",
                "doc": "The end date for this event (as UTC). Will be set automatically if\na {@link #model} is passed. May be set independently\nof any attached {@link #model}.\n",
                "default": "null"
            },
            {
                "name": "mode",
                "type": "String",
                "doc": "The display mode for this event. Possible options are:\n- `weekspan`\n- `weekinline`\n- `day`\n",
                "default": "null"
            },
            {
                "name": "model",
                "type": "Ext.calendar.model.EventBase",
                "doc": "A backing model for this widget.\n",
                "default": "null"
            },
