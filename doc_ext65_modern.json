    "Ext.calendar.panel.AbstractPanel": {
        "type": "Ext.calendar.panel.AbstractPanel",
        "doc": "A base class for a calendar panel that allows switching between views.\n",
        "configs": [
            {
                "name": "menuButton",
                "type": "Object",
                "doc": "The configuration for the menu button in {@link #compact} mode.\n",
                "default": "{\n    xtype: 'button',\n    iconCls: Ext.baseCSSPrefix + 'fa fa-bars'\n}"
