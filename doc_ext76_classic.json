    "Ext.pivot.result.Collection": {
        "type": "Ext.pivot.result.Collection",
        "doc": "This class stores the matrix results. When the pivot component uses the\n{@link Ext.pivot.matrix.Local} matrix then this class does\ncalculations in the browser.\n",
