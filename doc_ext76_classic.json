    "Ext.grid.selection.Selection": {
        "type": "Ext.grid.selection.Selection",
        "doc": "Base class for selections which may be of three subtypes:\n\n- {@link Ext.grid.selection.Cells Cells} A rectangular range of cells defined by a start\n  record/column and an end record/column.\n- {@link Ext.grid.selection.Rows Rows} An array of records.\n- {@link Ext.grid.selection.Columns Columns} An array of columns in which all records\n  are included.\n",
        "configs": [],
        "events": []
    },
    "Ext.grid.selection.SelectionExtender": {
