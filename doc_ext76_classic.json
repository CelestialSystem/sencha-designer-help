    "Ext.fx.target.Component": {
        "type": "Ext.fx.target.Component",
        "doc": "\nThis class represents a animation target for a {@link Ext.Component}. In general this class\nwill not be created directly, the {@link Ext.Component} will be passed to the animation and\nand the appropriate target will be created.\n",
        "configs": [],
        "events": []
    },
    "Ext.fx.target.CompositeElement": {
        "type": "Ext.fx.target.CompositeElement",
        "doc": "\nThis class represents a animation target for a {@link Ext.CompositeElement}. It allows\neach {@link Ext.dom.Element} in the group to be animated as a whole. In general this class\nwill not be created directly, the {@link Ext.CompositeElement} will be passed to the animation\nand and the appropriate target will be created.\n",
        "configs": [],
        "events": []
    },
    "Ext.fx.target.CompositeElementCSS": {
        "type": "Ext.fx.target.CompositeElementCSS",
        "doc": "\nThis class represents a animation target for a {@link Ext.CompositeElement}, where the\nconstituent elements support CSS based animation. It allows each {@link Ext.dom.Element} in\nthe group to be animated as a whole. In general this class will not be created directly,\nthe {@link Ext.CompositeElement} will be passed to the animation and the appropriate target\nwill be created.\n",
        "configs": [],
        "events": []
    },
    "Ext.fx.target.CompositeSprite": {
        "type": "Ext.fx.target.CompositeSprite",
        "doc": "\nThis class represents a animation target for a {@link Ext.draw.sprite.Composite}. It allows\neach {@link Ext.draw.sprite.Sprite} in the group to be animated as a whole. In general this class\nwill not be created directly, the {@link Ext.draw.sprite.Composite} will be passed\nto the animation and and the appropriate target will be created.\n",
        "configs": [],
        "events": []
    },
    "Ext.fx.target.Element": {
        "type": "Ext.fx.target.Element",
        "doc": "\nThis class represents a animation target for an {@link Ext.dom.Element}. In general this class\nwill not be created directly, the {@link Ext.dom.Element} will be passed to the animation and\nand the appropriate target will be created.\n",
        "configs": [],
        "events": []
    },
    "Ext.fx.target.ElementCSS": {
        "type": "Ext.fx.target.ElementCSS",
        "doc": "\nThis class represents a animation target for an {@link Ext.dom.Element} that supports CSS\nbased animation. In general this class will not be created directly, the {@link Ext.dom.Element}\nwill be passed to the animation and the appropriate target will be created.\n",
        "configs": [],
