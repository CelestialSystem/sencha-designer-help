            {
                "name": "baseRotation",
                "type": "Number",
                "doc": "The starting rotation of the angular axis.\n",
                "default": "'number'"
            },
            {
                "name": "centerX",
                "type": "Number",
                "doc": "The central point of the angular axis on the x-axis.\n",
                "default": "'number'"
            },
            {
                "name": "centerY",
                "type": "Number",
                "doc": "The central point of the angular axis on the y-axis.\n",
