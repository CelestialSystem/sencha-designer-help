            {
                "name": "grid",
                "type": "Boolean",
                "doc": "'true' if the axis has a grid.\n",
                "default": "'bool'"
            },
            {
                "name": "label",
                "type": "Object",
                "doc": "\nThe label configuration object for the Axis. This object may include style attributes\nlike `spacing`, `padding`, `font` that receives a string or number and\nreturns a new string with the modified values.\n",
                "default": "null"
            },
            {
                "name": "labelOffset",
                "type": "Number",
                "doc": "The distance between the label and the edge of a major tick.\nOnly applicable for 'gauge' and 'angular' axes.\n",
                "default": "10"
            },
            {
                "name": "layout",
                "type": "Object|Ext.chart.axis.layout.Layout",
                "doc": "The layout configuration used by\nthe axis.\n",
                "default": "null"
            },
            {
                "name": "length",
                "type": "Number",
                "doc": "The total length of the axis.\n",
                "default": "'number'"
