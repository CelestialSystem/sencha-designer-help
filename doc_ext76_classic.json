    "Ext.d3.axis.Data": {
        "type": "Ext.d3.axis.Data",
        "doc": "`{@link Ext.d3.axis.Data}` is an {@link Ext.d3.axis.Axis} that holds extra information\nneeded for use with stores.\n",
