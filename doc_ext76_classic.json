    },
    "Ext.fx.target.Sprite": {
        "type": "Ext.fx.target.Sprite",
        "doc": "This class represents an animation target for a {@link Ext.draw.sprite.Sprite}. In general\nthis class will not be created directly, the {@link Ext.draw.sprite.Sprite} will be passed\nto the animation and and the appropriate target will be created.\n",
        "configs": [],
        "events": []
    },
    "Ext.fx.target.Target": {
        "type": "Ext.fx.target.Target",
        "doc": "\nThis class specifies a generic target for an animation. It provides a wrapper around a\nseries of different types of objects to allow for a generic animation API.\nA target can be a single object or a Composite object containing other objects that are\nto be animated. This class and it's subclasses are generally not created directly, the\nunderlying animation will create the appropriate Ext.fx.target.Target object by passing\nthe instance to be animated.\n\nThe following types of objects can be animated:\n\n- {@link Ext.fx.target.Component Components}\n- {@link Ext.fx.target.Element Elements}\n- {@link Ext.fx.target.Sprite Sprites}\n",
        "configs": [],
        "events": []
    },
    "Ext.Gadget": {
        "type": "Ext.Gadget",
        "doc": "\n`{@link Ext.Gadget}` is not actually a class but simply an alternate class name for `{@link Ext.Widget}`. It\nis used primarily for writing cross-toolkit widgets.\n\nIn the modern toolkit, `{@link Ext.Gadget}` is an alternate class name for Ext.Component in order\nto allow gadgets to be used as `items` in an Ext.Container.\n",
        "configs": [],
        "events": []
    },
    "Ext.GlobalEvents": {
        "type": "Ext.GlobalEvents",
        "doc": "An `{@link Ext.mixin.Observable Observable}` through which Ext fires global events.\n\nExt.on() and Ext.un() are shorthand for {@link #addListener} and {@link #removeListener}\non this Observable.  For example, to listen for the idle event:\n\n    Ext.on('idle', function() {\n        // do something\n    });\n",
        "configs": [],
        "events": [
            {
                "name": "added",
                "doc": "Fires when a Component is added to a Container.\n"
            },
            {
                "name": "beforeresponsiveupdate",
                "doc": "Fires before {@link Ext.mixin.Responsive} perform any updates in response to\ndynamic changes. This is prior to refreshing `responsiveFormulas`.\n"
            },
            {
                "name": "beforeroute",
                "doc": "\nFires when a route is about to be executed. This allows pre-processing to add additional\n{@link Ext.route.Action#before before} or {@link Ext.route.Action#action action} handlers\nwhen the {@link Ext.route.Action Action} is run.\n\nThe route can be prevented from executing by returning `false` in a listener\nor executing the {@link Ext.route.Action#stop stop} method on the action.\n"
            },
            {
                "name": "beforerouteexit",
                "doc": "\nFires when a route is being exited meaning when a route\nwas executed but no longer matches a token in the current hash.\n\nThe exit handlers can be prevented from executing by returning `false` in a listener\nor executing the {@link Ext.route.Action#stop stop} method on the action.\n"
            },
            {
                "name": "beforeroutes",
                "doc": "\nFires when the hash has changed and before any routes are executed. This allows\npre-processing to add additional {@link Ext.route.Action#before before} or\n{@link Ext.route.Action#action action} handlers when the\n{@link Ext.route.Action Action} is run.\n\nRoute execution can be prevented by returning `false` in the listener\nor executing the {@link Ext.route.Action#stop stop} method on the action.\n"
            },
            {
                "name": "beginresponsiveupdate",
                "doc": "Fires when {@link Ext.mixin.Responsive} is about to perform updates in response to\ndynamic changes. At this point all `responsiveFormulas` have been refreshed.\n"
            },
            {
                "name": "collapse",
                "doc": "Fires when a Component is collapsed (e.g., a panel).\n"
            },
            {
                "name": "expand",
                "doc": "Fires when a Component is expanded (e.g., a panel).\n"
            },
            {
                "name": "hide",
                "doc": "Fires when a Component is hidden.\n"
            },
            {
                "name": "idle",
                "doc": "Fires when an event handler finishes its run, just before returning to\nbrowser control.\n\nThis includes DOM event handlers, Ajax (including JSONP) event handlers,\nand {@link Ext.util.TaskRunner TaskRunners}\n\nWhen called at the tail of a DOM event, the event object is passed as the\nsole parameter.\n\nThis can be useful for performing cleanup, or update tasks which need to\nhappen only after all code in an event handler has been run, but which\nshould not be executed in a timer due to the intervening browser\nreflow/repaint which would take place.\n"
            },
            {
                "name": "mousedown",
                "doc": "A mousedown listener on the document that is immune to stopPropagation()\nused in cases where we need to know if a mousedown event occurred on the\ndocument regardless of whether some other handler tried to stop it.  An\nexample where this is useful is a menu that needs to be hidden whenever\nthere is a mousedown event on the document.\n"
            },
            {
                "name": "mouseup",
                "doc": "A mouseup listener on the document that is immune to stopPropagation()\nused in cases where we need to know if a mouseup event occurred on the\ndocument regardless of whether some other handler tried to stop it.  An\nexample where this is useful is a component which enters a \"pressed\" state\nupon mousedown, and needs to exit that state even if the mouse exits\nbefore being released.\n"
            },
            {
                "name": "onlinechange",
                "doc": "Fires when the online status of the page changes. See {@link Ext#method-isOnline}\n"
            },
            {
                "name": "removed",
                "doc": "Fires when a Component is removed from a Container.\n"
            },
            {
                "name": "resize",
                "doc": "Fires when the browser window is resized.  To avoid running resize handlers\ntoo often resulting in sluggish window resizing, the resize event uses a buffer\nof 100 milliseconds in the Classic toolkit, and fires on animation frame\nin the Modern toolkit.\n"
            },
            {
                "name": "responsiveupdate",
                "doc": "Fires after {@link Ext.mixin.Responsive} has performed updates in response to\ndynamic changes.\n"
            },
            {
                "name": "resumelayouts",
                "doc": "Fires after global layout processing has been resumed in {@link\nExt.Component#resumeLayouts}.\n"
            },
            {
                "name": "routereject",
                "doc": "\nFires when a route was rejected from either a before action,\n{@link Ext.GlobalEvents#beforeroutes} event or {@link Ext.GlobalEvents#beforeroute} event.\n"
            },
            {
                "name": "show",
                "doc": "Fires when a Component is shown.\n"
            }
        ]
    },
