    "Ext.chart.axis.sprite.Axis": {
        "type": "Ext.chart.axis.sprite.Axis",
        "doc": "\nThe axis sprite. Currently all types of the axis will be rendered with this sprite.\n",
        "configs": [
            {
                "name": "axis",
                "type": "Ext.chart.axis.Axis",
                "doc": "The axis represented by this sprite.\n",
                "default": "null"
