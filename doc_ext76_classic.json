    "Ext.grid.NavigationModel": {
        "type": "Ext.grid.NavigationModel",
        "doc": "This class listens for key events fired from a {@link Ext.grid.Panel GridPanel},\nand moves the currently focused item by adding the class {@link #focusCls}.\n",
        "configs": [],
        "events": [
            {
                "name": "cellactivate",
                "doc": "Fired when a cell is activated while in actionable mode\n"
