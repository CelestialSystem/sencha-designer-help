            {
                "name": "max",
                "type": "Number",
                "doc": "The maximum value of the axis.\n{@link #min} and `{@link #cfg!max max}` attributes represent the effective range of the axis\nafter segmentation, layout, and range reconciliation between axes.\n",
                "default": "'number'"
            },
            {
                "name": "min",
                "type": "Number",
                "doc": "The minimum value of the axis.\n`{@link #cfg!min min}` and {@link #max} attributes represent the effective range of the axis\nafter segmentation, layout, and range reconciliation between axes.\n",
                "default": "'number'"
            },
            {
                "name": "minorTicks",
                "type": "Boolean",
                "doc": "'true' if the axis has sub ticks.\n",
                "default": "'bool'"
            },
            {
                "name": "minorTickSize",
                "type": "Number",
                "doc": "The length of the minor ticks.\n",
                "default": "'number'"
            },
            {
                "name": "minStepSize",
                "type": "Number",
                "doc": "The minimum step size between ticks.\n",
