    "Ext.ux.form.MultiSelect": {
        "type": "Ext.ux.form.MultiSelect",
        "doc": "A control that allows selection of multiple items in a list.\n",
        "configs": [
            {
                "name": "allowBlank",
                "type": "Boolean",
                "doc": "`false` to require at least one item in the list\nto be selected, `true` to allow no selection.\n",
                "default": "true"
            },
            {
                "name": "appendOnly",
                "type": "String",
                "doc": "`true` if the list should only allow append drops\nwhen drag/drop is enabled. This is useful for lists which are sorted.\n",
                "default": "false"
            },
            {
                "name": "blankText",
                "type": "String",
                "doc": "Default text displayed when the control\ncontains no items.\n",
                "default": "\"This field is required\""
            },
            {
                "name": "ddReorder",
                "type": "Boolean",
                "doc": "Whether the items in the MultiSelect list are drag/drop\nreorderable.\n",
                "default": "false"
            },
            {
                "name": "delimiter",
                "type": "String",
                "doc": "The string used to delimit the selected values when\n{@link #getSubmitValue submitting} the field as part of a form. If you wish to have\nthe selected values submitted as separate parameters rather than a single\ndelimited parameter, set this to `null`.\n",
                "default": "\",\""
            },
            {
                "name": "displayField",
                "type": "String",
                "doc": "Name of the desired display field in the dataset.\n",
                "default": "\"text\""
            },
            {
                "name": "dragGroup",
                "type": "String",
                "doc": "The ddgroup name for the MultiSelect DragZone.\n",
                "default": "\"\""
            },
            {
                "name": "dragText",
                "type": "String",
                "doc": "The text to show while dragging items.\n{0} will be replaced by the number of items. {1} will be replaced by the plural\nform if there is more than 1 item.\n",
                "default": "\"{0} Item{1}\""
            },
            {
                "name": "dropGroup",
                "type": "String",
                "doc": "The ddgroup name for the MultiSelect DropZone.\n",
                "default": "\"\""
            },
            {
                "name": "listConfig",
                "type": "Object",
                "doc": "An optional set of configuration properties that will be passed to the\n{@link Ext.view.BoundList}'s constructor. Any configuration that is valid for BoundList\ncan be included.\n"
            },
            {
                "name": "maxSelections",
                "type": "Number",
                "doc": "Maximum number of selections allowed.\n",
                "default": "Number.MAX_VALUE"
            },
            {
                "name": "maxSelectionsText",
                "type": "String",
                "doc": "Validation message displayed when {@link #maxSelections} is not met\nThe {0} token will be replaced by the value of {@link #maxSelections}.\n",
                "default": "\"Maximum {0}item(s) allowed\""
            },
            {
                "name": "minSelections",
                "type": "Number",
                "doc": "Minimum number of selections allowed.\n",
                "default": "0"
            },
            {
                "name": "minSelectionsText",
                "type": "String",
                "doc": "Validation message displayed when {@link #minSelections} is not met.\nThe {0} token will be replaced by the value of {@link #minSelections}.\n",
                "default": "\"Minimum {0}item(s) required\""
            },
            {
                "name": "pageSize",
                "type": "Number",
                "doc": "The number of items to advance on pageUp and pageDown\n",
                "default": "10"
            },
            {
                "name": "store",
                "type": "Ext.data.Store/Array",
                "doc": "The data source to which this MultiSelect is bound\n(defaults to `undefined`).\nAcceptable values for this property are:\n<div class=\"mdetail-params\"><ul>\n<li><b>any {@link Ext.data.Store Store} subclass</b></li>\n<li><b>an Array</b> : Arrays will be converted to a {@link Ext.data.ArrayStore} internally.\n<div class=\"mdetail-params\"><ul>\n<li><b>1-dimensional array</b> : (e.g., <tt>['Foo','Bar']</tt>)<div class=\"sub-desc\">\nA 1-dimensional array will automatically be expanded (each array item will be the combo\n{@link #valueField value} and {@link #displayField text})</div></li>\n<li><b>2-dimensional array</b> : (e.g., `[['f','Foo'],['b','Bar']]`)<div class=\"sub-desc\">\nFor a multi-dimensional array, the value in index 0 of each item will be assumed to be\nthe combo {@link #valueField value}, while the value at index 1 is assumed to be the combo\n{@link #displayField text}.\n</div></li></ul></div></li></ul></div>\n"
            },
            {
                "name": "tbar",
                "type": "Object/Array",
                "doc": "An optional toolbar to be inserted at the top of the control's\nselection list. This can be a {@link Ext.toolbar.Toolbar} object, a toolbar config,\nor an array of buttons/button configs to be added to the toolbar.\nSee {@link Ext.panel.Panel#tbar}.\n"
            },
            {
                "name": "title",
                "type": "String",
                "doc": "A title for the underlying panel.\n",
                "default": "\"\""
            },
            {
                "name": "valueField",
                "type": "String",
